#include<bits/stdc++.h>



#include <iostream>

using namespace std;
#define ll long long 

int main()
{
    int t;
    cin>>t;
    while(t--){
       ll n;
       cin>>n;
       ll ans=1;
       ll  mx=0;
       for(int i=0;i<n;i++){
           ll a,b;
           cin>>a>>b;
           if(a<=10){
               if(b>mx){
                   ans=i+1;
                   mx=b;
               }
           }
       }
       cout<<ans<<endl;
        
    }

    return 0;
}
